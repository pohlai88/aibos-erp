{
  "name": "@aibos/accounting",
  "version": "0.1.0",
  "description": "Accounting domain for AI-BOS ERP",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.types.json",
    "build:js": "tsup",
    "build": "pnpm run build:types && pnpm run build:js",
    "dev": "tsup --watch",
    "format": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aibos/eventsourcing": "workspace:*",
    "@types/express": "^5.0.3",
    "express": "^5.1.0",
    "uuid": "9.0.0",
    "yaml": "^2.8.1",
    "zod": "3.22.0"
  },
  "devDependencies": {
    "@types/uuid": "9.0.0",
    "@vitest/ui": "2.0.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "2.0.0"
  }
}
