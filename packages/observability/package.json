{
  "name": "@aibos/observability",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/src/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:types": "tsc -p tsconfig.types.json",
    "build:js": "tsup",
    "build": "pnpm run build:types && pnpm run build:js",
    "format": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo 'No tests yet'",
    "dep:check": "depcruise --validate --config ../../.dependency-cruiser.js --output-type text src",
    "dev": "tsup --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "eslint": "9.36.0",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
