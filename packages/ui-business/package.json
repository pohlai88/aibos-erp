{
  "name": "@aibos/ui-business",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./cfo-dashboard": {
      "types": "./dist/types/src/cfo-dashboard/index.d.ts",
      "import": "./dist/cfo-dashboard/index.js",
      "require": "./dist/cfo-dashboard/index.cjs",
      "default": "./dist/cfo-dashboard/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/types/src/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:types": "tsc -p tsconfig.types.json",
    "build:js": "tsup",
    "build": "pnpm run build:types && pnpm run build:js",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aibos/accounting-contracts": "workspace:*",
    "@aibos/ui": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "eslint": "9.36.0",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
