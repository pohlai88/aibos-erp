{
    "name": "@aibos/eventsourcing",
    "version": "0.1.0",
    "description": "Event Sourcing infrastructure for AI-BOS ERP",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "format": "prettier -w .",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "uuid": "9.0.0",
        "zod": "3.22.0"
    },
    "devDependencies": {
        "@types/pg": "8.10.0",
        "@types/uuid": "9.0.0",
        "@vitest/ui": "2.0.0",
        "eslint": "9.36.0",
        "prettier": "3.6.2",
        "tsup": "8.5.0",
        "typescript": "5.9.2",
        "vitest": "2.0.0"
    },
    "peerDependencies": {
        "pg": "8.16.3",
        "kafkajs": "2.2.4"
    }
}