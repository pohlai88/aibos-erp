{
  "name": "@aibos/ui",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo 'No tests yet'",
    "build": "tsup",
    "dep:check": "depcruise --validate --config ../../.dependency-cruiser.js --output-type text src",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "dx:stage1": "prettier --write . && eslint . --fix",
    "dx:stage2": "knip --fix --no-gitignore || true && ts-prune || true",
    "dx:security": "eslint . --rule 'security/detect-object-injection:error'",
    "dx": "pnpm run build && tsc --noEmit && eslint . --max-warnings=0"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "lucide-react": "0.400.0",
    "tailwind-merge": "2.6.0"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "0.1.0",
    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/typography": "0.5.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "autoprefixer": "10.4.0",
    "eslint": "9.36.0",
    "postcss": "8.4.0",
    "prettier": "3.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.0",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
